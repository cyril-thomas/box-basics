{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost:8080/home"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "img.logo"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Get Started"
      }
    },
    {
      "type": "pause",
      "waitTime": "1000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "REGISTER NOW!"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "firstName"
      },
      "text": "Cyril"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "lastName"
      },
      "text": "Thomas"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "email"
      },
      "text": "cyril.satyam@gmail.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "phone"
      },
      "text": "8016969697"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "city"
      },
      "text": "SLC"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "zip"
      },
      "text": "84111"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "date-sc"
      },
      "text": "12-09-1984"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.btn.btn-primary"
      }
    },
    {
      "type": "pause",
      "waitTime": "1000"
    },
    {
      "type": "verifyTextPresent",
      "text": "Thanks for registering, please come visit us!"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "HOME"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}