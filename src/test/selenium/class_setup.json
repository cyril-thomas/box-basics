{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost:8080/home"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Admin"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "username"
      },
      "text": "johnwayne@woddojo.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "password"
      },
      "text": "hello"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div/div[4]/div[5]/div/div[2]/a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Add new"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "title"
      },
      "text": "Test selenium class"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//select[@id='coach']//option[2]"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "description"
      },
      "text": "Test selenium class"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.btn.btn-primary"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Test selenium class"
    },
    {
      "type": "verifyTextPresent",
      "text": "Hello John!"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Logout"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}