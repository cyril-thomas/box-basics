{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost:8080/"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "img.logo"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Let's Get In Touch!"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "name"
      },
      "text": "Cyril Thomas"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "aphone"
      },
      "text": "8016969697"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "aemail"
      },
      "text": "cyril.satyam@gmail.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "message"
      },
      "text": "Testing with selenium"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "button.btn.btn-success"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}